}
<div class="feature">
    <div class="feature-header">
        <div class="feature-title">Văn học</div>
        
    </div>
    <div class="feature-products">
        <div class="row row-cols-2 row-cols-sm-3 row-cols-md-4 row-cols-lg-5 p-2 justify-content-center">
            @foreach (Fahasa.Models.Product item in Model.Literatures)
            {

                @Html.Partial("Client/_Product", item)
            }
        </div>
        <div class="tabs-xem-them fhs_bar_bottom mobile_link" style="">
            <a  href="/Products/Search?cate=Văn học">Xem Thêm</a>
        </div>
